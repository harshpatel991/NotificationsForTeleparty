{
   "content_scripts": [
      {
         "js": [
            "background.js"
         ],
         "matches": [
            "https://redirect.teleparty.com/*"
         ],
         "all_frames": true
      }
   ],
   "description": "Adds sound and Chrome notifications to Teleparty",
